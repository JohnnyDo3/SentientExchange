{
  "name": "agentmarket-mcp",
  "version": "1.0.0",
  "description": "AI Agent Service Marketplace via MCP + x402",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cd web && npm install && npm run build && cd ..",
    "start": "node start-railway.js",
    "start:all": "node start-all.js",
    "start:mcp": "node dist/index.js",
    "start:api": "node dist/server/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "dev:api": "nodemon --watch src/api --exec ts-node src/api/apiServer.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/integration/e2e.test.ts",
    "test:e2e:real": "jest tests/e2e/real-payment-flow.test.ts",
    "test:e2e:true": "jest tests/e2e/true-e2e-payment-flow.test.ts --runInBand --verbose",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e:real",
    "seed": "ts-node scripts/seed-database.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "clean": "rm -rf dist",
    "prepare": "husky"
  },
  "keywords": [
    "mcp",
    "x402",
    "ai-agents",
    "marketplace",
    "payments"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@coinbase/coinbase-sdk": "^0.25.0",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@types/cookie-parser": "^1.4.10",
    "@types/pg": "^8.15.6",
    "axios": "^1.13.1",
    "bs58": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mcpay": "^0.1.12",
    "pg": "^8.16.3",
    "siwe": "^3.0.0",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/express-rate-limit": "^6.0.2",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
