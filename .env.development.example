# Development Environment Variables
# Copy this file to .env for local development

# CDP Wallet Configuration (REQUIRED for payment functionality)
CDP_API_KEY_NAME=your-cdp-api-key-name
CDP_API_KEY_PRIVATE_KEY=your-cdp-private-key

# Network (base-sepolia for testing, base for production)
NETWORK=base-sepolia

# Database
DATABASE_PATH=./data/agentmarket.db

# Server Configuration
NODE_ENV=development
PORT=3000
API_PORT=8081

# Logging
LOG_LEVEL=debug

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_X402_PAYMENTS=true

# Development URLs
# Leave empty for local development, or set to your Railway preview URL
RAILWAY_URL=

# Optional: Database connection for PostgreSQL (if not using SQLite)
# DATABASE_URL=postgresql://user:password@localhost:5432/agentmarket_dev
